<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.wieskamp.puzzeltocht" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Wieskamp Puzzeltocht</name>
    <description>
        Vind met deze app alle iBeacons die zijn verstopt op de camping.
    </description>
    <author email="rob@wieskamp.nl" href="http:www.wieskamp.nl">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
</widget>
